<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AndroidCourseWork" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/register_bk_color</item>
        <item name="colorPrimaryVariant">@color/register_bk_color</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
<!--        <item name="" android:color="@color/pink" android:state_checked="true" />-->
    </style>
    <style name="CustomPopupTheme" parent="ThemeOverlay.AppCompat.Dark">
        <item name="android:colorControlActivated">@color/black</item>
        <item name="android:colorControlHighlight">@color/register_bk_color</item>
        <item name="android:colorControlNormal">@color/white</item>
    </style>
</resources>